            </DoubleVectorProperty>
            <DoubleVectorProperty name="Highest pressure"
                command="setUpperPressure"
                number_of_elements="1"
                default_values="1033.3">
                <DoubleRangeDomain name="range" min="8.12409" max="1033.3" />
            </DoubleVectorProperty>

            <DoubleVectorProperty name="Starting Temperature"
                command="setLowerTemperature"
                number_of_elements="1"
                default_values="184.492">
                <DoubleRangeDomain name="range" min="184.492" max="317.236" />
            </DoubleVectorProperty>
            <DoubleVectorProperty name="Highest Temperature"
                command="setUpperTemperature"
                number_of_elements="1"
                default_values="317.236">
                <DoubleRangeDomain name="range" min="184.492" max="317.236" />
            </DoubleVectorProperty>

            <DoubleVectorProperty name="Starting Time"
                command="setLowerDate"
                number_of_elements="1"
