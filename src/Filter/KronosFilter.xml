<ServerManagerConfiguration>
    <ProxyGroup name="filters">
        <SourceProxy name="CustomFilter" class="vtkGlobeFilter" label="My Custom Filter">
            <Documentation
                long_help="Create point attribute array by projecting points onto an elevation vector."
                short_help="Create a point array representing elevation.">
            </Documentation>
            <InputProperty
                name="Input"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPolyData"/>
                </DataTypeDomain>
            </InputProperty>
            <Property name="reloadTheStuff"
            animateable="1"
            command="reloadStuff" />
        </SourceProxy>
        <SourceProxy name="SpericalToCartesianFilter" class="SpericalToCartesianFilter" label="test">
            <Documentation
                long_help="nopevation vector."
                short_help="Create a point array representing elevation.">
            </Documentation>
            <InputProperty
                name="Input"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPolyData"/>
                </DataTypeDomain>
            </InputProperty>
        </SourceProxy>
    </ProxyGroup>
</ServerManagerConfiguration>