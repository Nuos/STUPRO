<ServerManagerConfiguration>
    <ProxyGroup name="filters">
            <SourceProxy name="SphericalToCartesianFilter" class="SphericalToCartesianFilter" label="Kronos Spherical to Cartesian">
            <Documentation
                long_help="This filter can convert GPS data (longitude/latitude/height) to cartesian coordinates in order to 'wrap' the data around a globe."
                short_help="Convert GPS-Data to cartesian coordinates">
            </Documentation>
            <InputProperty
                name="Input"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPointSet"/>
		    <DataType value="vtkImageData"/>
		    <DataType value="vtkRectilinearGrid"/>
                </DataTypeDomain>
            </InputProperty>
	    <IntVectorProperty name="transform"
            			command="setTransform"
            			number_of_elements="1"
            			default_values="1">
		<BooleanDomain name="bool"/>
	    </IntVectorProperty>
	    <Hints>
		<ShowInMenu category="Kronos" />
	    </Hints>
        </SourceProxy>

        <SourceProxy name="PrecipitationTypeFilter" class="PrecipitationTypeFilter" label="Kronos Precipitation Type Filter">
            <Documentation
                long_help="This filter can extract data from precipitation point sets read by a Kronos reader depending on the precipitation type of each point."
                short_help="Extract data based on the type of precipitation">
            </Documentation>
            <InputProperty
                name="Input"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPointSet"/>
                </DataTypeDomain>
            </InputProperty>
            <IntVectorProperty name="Display precipitation of undefined type"
             command="enableUndefined"
             number_of_elements="1"
             default_values="1">
                <BooleanDomain name="bool"/>
            </IntVectorProperty>
            <IntVectorProperty name="Display rain"
             command="enableRain"
             number_of_elements="1"
             default_values="1">
                <BooleanDomain name="bool"/>
            </IntVectorProperty>
            <IntVectorProperty name="Display snow"
             command="enableSnow"
             number_of_elements="1"
             default_values="1">
                <BooleanDomain name="bool"/>
            </IntVectorProperty>
            <IntVectorProperty name="Display sleet"
             command="enableSleet"
             number_of_elements="1"
             default_values="1">
                <BooleanDomain name="bool"/>
            </IntVectorProperty>
            <IntVectorProperty name="Display hail"
             command="enableHail"
             number_of_elements="1"
             default_values="1">
                <BooleanDomain name="bool"/>
            </IntVectorProperty>
            <Hints>
              <ShowInMenu category="Kronos"/>
            </Hints>
        </SourceProxy>

        <SourceProxy name="MIPASFilter" class="SciVisFilter" label="MIPASFilter">
          <Documentation
            long_help="Use the SciVis Filter to solve every problem related to the SciVis-Contest 2014 with MIPAS Data"
            short_help="Solve SciVis-Contest with this filter">
          </Documentation>
          <InputProperty
            name="MIPAS Input"
            command="SetInputConnection">
            <ProxyGroupDomain name="groups">
              <Group name="sources"/>
              <Group name="filters"/>
            </ProxyGroupDomain>
            <DataTypeDomain name="input_type">
              <DataType value="vtkPolyData"/>
              <DataType value="vtkCellData"/>
              <DataType value="vtkUnstructuredGrid"/>
              <DataType value="vtkPointSet"/>
            </DataTypeDomain>
          </InputProperty>

          <DoubleVectorProperty name="Start-Date"
           command="setLowerMIPAS"
           number_of_elements="1"
           default_values="368156000.0">
          <DoubleRangeDomain name="range" min="368156000.0" max="360205000.0" />
          </DoubleVectorProperty>

          <DoubleVectorProperty name="End-Date"
           command="setUpperMIPAS"
           number_of_elements="1"
           default_values="368156000.0">
          <DoubleRangeDomain name="range" min="368156000.0" max="360205000.0" />
          </DoubleVectorProperty>

          <Hints>
            <ShowInMenu category="SciVis"/>
          </Hints>
        </SourceProxy>

        <SourceProxy name="AIRSFilter" class="SciVisFilter" label="AIRSFilter">
          <Documentation
            long_help="Use the SciVis Filter to solve every problem related to the SciVis-Contest 2014 with AIRS Data"
            short_help="Solve SciVis-Contest with this filter">
          </Documentation>
          <InputProperty
            name="AIRS Input"
            command="SetInputConnection">
            <ProxyGroupDomain name="groups">
              <Group name="sources"/>
              <Group name="filters"/>
            </ProxyGroupDomain>
            <DataTypeDomain name="input_type">
              <DataType value="vtkPolyData"/>
              <DataType value="vtkCellData"/>
              <DataType value="vtkUnstructuredGrid"/>
              <DataType value="vtkPointSet"/>
            </DataTypeDomain>
          </InputProperty>
          <DoubleVectorProperty name="Start-Date"
           command="setLowerAIRS"
           number_of_elements="1"
           default_values="360029000.0">
          <DoubleRangeDomain name="range" min="360029000.0" max="360072000.0" />
          </DoubleVectorProperty>

          <DoubleVectorProperty name="End-Date"
           command="setUpperAIRS"
           number_of_elements="1"
           default_values="360029000.0">
          <DoubleRangeDomain name="range" min="360029000.0" max="360072000.0" />
          </DoubleVectorProperty>
          <Hints>
            <ShowInMenu category="SciVis"/>
          </Hints>
        </SourceProxy>

        <SourceProxy name="CLaMSFilter" class="SciVisFilter" label="CLaMSFilter">
          <Documentation
            long_help="Use the SciVis Filter to solve every problem related to the SciVis-Contest 2014 with AIRS Data"
            short_help="Solve SciVis-Contest with this filter">
          </Documentation>
          <InputProperty
            name="CLaMS Input"
            command="SetInputConnection">
            <ProxyGroupDomain name="groups">
              <Group name="sources"/>
              <Group name="filters"/>
            </ProxyGroupDomain>
            <DataTypeDomain name="input_type">
              <DataType value="vtkPolyData"/>
              <DataType value="vtkCellData"/>
              <DataType value="vtkUnstructuredGrid"/>
              <DataType value="vtkPointSet"/>
            </DataTypeDomain>
          </InputProperty>
          <DoubleVectorProperty name="Start-Date"
           command="setLowerCLaMS"
           number_of_elements="1"
           default_values="361217000.0">
          <DoubleRangeDomain name="range" min="361217000.0" max="368582000.0" />
          </DoubleVectorProperty>

          <DoubleVectorProperty name="End-Date"
           command="setUpperCLaMS"
           number_of_elements="1"
           default_values="361217000.0">
          <DoubleRangeDomain name="range" min="361217000.0" max="368582000.0" />
          </DoubleVectorProperty>

          <Hints>
            <ShowInMenu category="SciVis"/>
          </Hints>
        </SourceProxy>

        </ProxyGroup>
        <ParaViewFilters>
        <Category name="Kronos" menu_label="Kronos">
          <Filter name="SphericalToCartesianFilter" />
          <Filter name="PrecipitationTypeFilter" />
          <Filter name="FlightFilter" />
        </Category>

        <Category name="SciVis" menu_label="SciVis">
          <Filter name="MIPASFilter" />
          <Filter name="AIRSFilter" />
          <Filter name="CLaMSFilter" />
        </Category>
</ParaViewFilters>

</ServerManagerConfiguration>
