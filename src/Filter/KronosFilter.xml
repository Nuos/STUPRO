                default_values="247.64">
                <DoubleRangeDomain name="range" min="247.64" max="1447.37" />
            </DoubleVectorProperty>
            <DoubleVectorProperty name="Highest pot_Temperature"
                command="setUpperPotTemperature"
                number_of_elements="1"
                default_values="1447.37">
                <DoubleRangeDomain name="range" min="247.64" max="1447.37" />
            </DoubleVectorProperty>

            <DoubleVectorProperty name="Starting vorticity"
                command="setLowerVorticity"
