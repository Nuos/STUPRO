            <DoubleVectorProperty name="Highest Altitude"
                command="setUpperAltitude"
                number_of_elements="1"
                default_values="30.0">
                <DoubleRangeDomain name="range" min="4.516" max="30.0" />
            </DoubleVectorProperty>

            <IntVectorProperty name="Starting Range Orbit"
                command="setLowerOrbit"
                number_of_elements="1"
                default_values="48379">
                <IntRangeDomain name="range" min="48379" max="49701" />
            </IntVectorProperty>
            <IntVectorProperty name="Ending Range Orbit"
                command="setUpperOrbit"
                number_of_elements="1"
                default_values="49701">
                <IntRangeDomain name="range" min="48379" max="49701" />
            </IntVectorProperty>
            <DoubleVectorProperty name="Starting Time"
                command="setLower"
                number_of_elements="1"
                default_values="360205000.0">
                <DoubleRangeDomain name="range" min="360205000.0" max="368156000.0" />
            </DoubleVectorProperty>
            <DoubleVectorProperty name="Ending Time"
                command="setUpper"
                number_of_elements="1"
                default_values="368156000.0">
                <DoubleRangeDomain name="range" min="360205000.0" max="368156000.0" />
            </DoubleVectorProperty>

            <PropertyGroup label="Altitude">
                <Property name="Lowest Altitude" />
                <Property name="Highest Altitude" />
            </PropertyGroup>
            <PropertyGroup label="Orbit">
                <Property name="Starting Range Orbit" />
                <Property name="Ending Range Orbit" />
            </PropertyGroup>
            <PropertyGroup label="Time">
                <Property name="Starting Time" />
                <Property name="Ending Time" />
            </PropertyGroup>

            <Hints>
                <ShowInMenu category="SciVis"/>
            </Hints>
        </SourceProxy>
