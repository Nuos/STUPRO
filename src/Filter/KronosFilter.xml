                <DoubleRangeDomain name="range" min="361217000.0" max="368582000.0" />
            </DoubleVectorProperty>
            <DoubleVectorProperty name="Ending Time"
                command="setUpperDate"
                number_of_elements="1"
                default_values="368582000.0">
                <DoubleRangeDomain name="range" min="361217000.0" max="368582000.0" />
            </DoubleVectorProperty>

            <PropertyGroup label="Altitude">
                <Property name="Lowest Altitude" />
                <Property name="Highest Altitude" />
            </PropertyGroup>
            <PropertyGroup label="potential Temperature">
                <Property name="Starting pot_Temperature" />
                <Property name="Highest pot_Temperature" />
            </PropertyGroup>
            <PropertyGroup label="potential Vorticity">
                <Property name="Starting vorticity" />
                <Property name="Highest vorticity" />
            </PropertyGroup>
            <PropertyGroup label="Temperature">
                <Property name="Starting Temperature" />
                <Property name="Highest Temperature" />
            </PropertyGroup>
            <PropertyGroup label="Pressure">
                <Property name="Starting pressure" />
                <Property name="Highest pressure" />
            </PropertyGroup>
            <PropertyGroup label="Time">
