<ServerManagerConfiguration>
    <ProxyGroup name="filters">
          <SourceProxy name="SpericalToCartesianFilter" class="SpericalToCartesianFilter" label="Kronos Spherical to Cartesian">
            <Documentation
                long_help="nopevation vector."
                short_help="Create a point array representing elevation.">
            </Documentation>
            <InputProperty
                name="Input"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkPointSet"/>
                </DataTypeDomain>
            </InputProperty>
        </SourceProxy>
    </ProxyGroup>
</ServerManagerConfiguration>
