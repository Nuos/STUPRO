<ServerManagerConfiguration>
  <ProxyGroup name="sources">
      <SourceProxy name="MyPNGReader" class="vtkCityReader" label="CSVReader">
          <Documentation
              long_help="Read a  myCSV file."
              short_help="Read a myCSV file.">
          </Documentation>
          <StringVectorProperty
              name="FileName"
              animateable="0"
              command="SetFileName"
              number_of_elements="1">
              <FileListDomain name="files"/>
              <Documentation>
                  This property specifies the file name for the PNG reader.
              </Documentation>
          </StringVectorProperty>

          <Hints>
              <ReaderFactory extensions="meinCSV"
              file_description="MEINEIGENESCSVORMAT" />
          </Hints>
      </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
